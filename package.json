
{
    "scripts": {
        "backend:install": "cd udagram-api && npm install --force",
        "frontend:install": "cd udagram-frontend && npm install --force",
        "backend:build": "cd udagram-api && npm run build",
        "frontend:build": "cd udagram-frontend && set NODE_OPTIONS=--openssl-legacy-provider && npm run build",
        "frontend:deploy": "cd udagram-frontend && npm run deploy",
        "backend:deploy": "cd udagram-api && npm run deploy",
        "backend:test": "cd udagram-api && npm run test",
        "frontend:test": "cd udagram-frontend && npm run test"
    },
    "devDependencies": {}
}